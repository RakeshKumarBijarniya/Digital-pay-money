{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "src/constants/ImagePath.jsx",
    "src/component/atoms/CustomHeader.jsx",
    "src/app/services/LoginServices.jsx",
    "src/app/(auth)/Login.jsx",
    "src/app/(main)/Profile.jsx",
    "src/app/(main)/UpdateProfile.jsx",
    "src/app/services/ApiServices.jsx",
    "src/app/(main)/(tabs)/_layout.jsx",
    "src/app/(main)/WaterPay.jsx",
    "src/app/(main)/(tabs)/History.jsx",
    "src/app/(main)/(tabs)/index.jsx",
    "src/component/molecules/RecentsPay.jsx",
    "src/app/(main)/BarCodeScanner.jsx"
  ]
}
